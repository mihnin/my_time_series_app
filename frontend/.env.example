# ==============================================
# Time Series Analysis Frontend2 Environment Variables
# ==============================================
# Copy this file to .env and fill in your actual values
# ==============================================

# Backend API Configuration
# Address and port where the FastAPI backend is running
VITE_BACKEND_ADDRESS=http://localhost
VITE_BACKEND_PORT=8000

# Alternative API Base URL format (used by some components)
VITE_API_BASE_URL=http://localhost:8000

# Development Settings
VITE_NODE_ENV=development

# Optional: API timeout settings (in milliseconds)
VITE_API_TIMEOUT=30000

# Environment
NODE_ENV=development
